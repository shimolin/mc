<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %> </title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>
<body>
	<div class="g">
		<div class="g-row">
    		<div class="g-12">
				<%= render 'layouts/header' %>
			</div>
		</div>
	</div>

				
	<div class="container">
		<div class="g">
  			<div class="g-row">
    			<div class="g-12">
					<%= yield %>		
				</div>
			</div>
		</div>
	</div>


	<div class="g">
  		<div class="g-row">
    		<div class="g-12">
				<%= render 'layouts/footer' %>
			</div>
		</div>
	</div>

</body>
</html>
